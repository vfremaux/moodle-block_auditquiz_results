

<!-- assigncourseform -->
<form id="assignform" method="post" action="{{{formurl}}}">
<div>
    <input type="hidden" name="sesskey" value="{{sesskey}}" />
    <input type="hidden" name="id" value="{{blockid}}" />
    <input type="hidden" name="qcatid" value="{{qcatid}}" />

    <table id="assigningcourse" class="admintable courseassigntable generaltable" cellspacing="0">
        <tr>
            <td id="existingcell">
                <p><label for="removeselect">{{extcoursesstr}}</label></p>
                {{{assignedcoursesselector}}}
            </td>

            <td id="buttonscell">
                <div id="addcontrols">
                    <input name="add" id="add" type="submit" value="{{{larrowstr}}}" title="{{titleadd}}" /><br />
                </div>

                <div id="removecontrols">
                    <input name="remove" id="remove" type="submit" value="{{{rarrowstr}}}" title="{{titleremove}}" />
                </div>
            </td>

            <td id="potentialcell">
                <p><label for="addselect">{{potcoursesstr}}</label></p>
                {{{potentialcoursesselector}}}
            </td>
        </tr>
    </table>
</div>
</form>
<!-- /assigncourseform -->