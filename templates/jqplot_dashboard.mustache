


<!-- jqplot_dashboard -->

<style type="text/css" media="screen">
    .jqplot-axis {
        font-size: 0.85em;
    }
    .jqplot-point-label {
        border: 1.5px solid #aaaaaa;
        padding: 1px 3px;
        background-color: #eeccdd;
    }
</style>

<div class="auditquiz-results-graph" id="id-auditquiz-{{userid}}-{{blockid}}">
    {{{plot}}}
</div>

{{#cansnapshot}}
<div class="auditquiz-graph-ctl">
    <input type="button" class="auditquiz-snapshot-btn" id="id-auditquiz-snapshot-{{userid}}-{{blockid}}" value="{{snapshotstr}}" />
    <div id="id-snapshot-feedback"></div>
</div>
{{#hassnapshots}}
<div class="auditquiz-snapshots" id="id-auditquiz-snapshots-container">
{{> block_auditquiz_results/snapshotlist }}
</div>
{{/hassnapshots}}
{{/cansnapshot}}

<!-- /jqplot_dashboard -->
